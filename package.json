{
   "name": "vanilla",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev": "vite --open --host",
      "build": "vite build",
      "preview": "vite preview",
      "sprite": " svgo --config ./svgo-cfg.cjs -f ./src/svgicons -o ./src/svgiconsmin && svg-symbol-sprite -i ./src/svgiconsmin -o ./public/img/icons/sprite.svg -c false",
      "webp": "webpconvert public/img -r true && rm public/img/*/**.{png,jpg,jpeg} && for f in public/img/*/**.webp"
   },
   "devDependencies": {
      "autoprefixer": "^10.4.15",
      "postcss": "^8.4.28",
      "svg-symbol-sprite": "^1.4.0",
      "svgo": "^3.0.2",
      "tailwindcss": "^3.3.3",
      "vite": "^4.4.5",
      "vite-plugin-html-inject": "^1.0.1",
      "vite-plugin-inspect": "^0.7.38",
      "webpconvert": "^3.0.1"
   }
}
